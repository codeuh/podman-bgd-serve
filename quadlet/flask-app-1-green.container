[Unit]
Description=Flask App 1 Green
After=network.target

[Container]
ContainerName=flask-app-1-green
Image=localhost:8082/flask-app-1
PodmanArgs=--tls-verify=false
PublishPort=5001:5000
AutoUpdate=registry

[Service]
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target